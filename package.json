{
  "name": "@jobbnorge/jn-ckeditors",
  "homepage": "/jn-ckeditors/",
  "private": false,
  "version": "2.5.0",
  "author": "CKSource",
  "description": "A custom CKEditor 5 build made by the CKEditor 5 online builder.",
